<UserControl x:Class="TabML.Editor.Tablature.BarLine"
             x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TabML.Editor.Tablature"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d" 
             d:DesignHeight="{DynamicResource BarLineHeight}" d:DesignWidth="300">
    <UserControl.Resources>
        <system:Double x:Key="BarLineHeight">12</system:Double>
    </UserControl.Resources>
    <StackPanel x:Name="ContentStack"
                Orientation="Horizontal"
                Height="{DynamicResource BarLineHeight}">
    </StackPanel>
</UserControl>
